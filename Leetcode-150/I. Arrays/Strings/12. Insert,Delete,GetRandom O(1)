#include<iostream>
#include<vector>
#include<unordered_map>
#include<cstdlib>
#include<ctime>
class RandomizedSet {
public:
    // to get O(1) access element : Vector DS is best
    // insert and delete at front /  beginning in vector takes O(n) time so we will use unordered_map
    vector<int> v;
    unordered_map<int,int> mp; // store element with its index of vector
    RandomizedSet() {
       srand(time(0));
    }
    
    bool insert(int val) {
       if (mp.find(val) == mp.end()) {
        v.push_back(val);
        mp[val] = v.size() - 1;
        return true;
       } else {
        return false;
       }
    }
    
    bool remove(int val) {
       if (mp.find(val)!= mp.end()) {
        //remove from vector first
        int lastElement = v.back();
        int index = mp[val];
        v[index] = lastElement;
        mp[lastElement] = index;
        v.pop_back();
        mp.erase(val);
        return true;
       } else {
        return false;
       }
    }
    
    
    int getRandom() {
     int randomIndex = rand() % v.size();
     return v[randomIndex];
    }
};

/**
 * Your RandomizedSet object will be instantiated and called as such:
 * RandomizedSet* obj = new RandomizedSet();
 * bool param_1 = obj->insert(val);
 * bool param_2 = obj->remove(val);
 * int param_3 = obj->getRandom();
 */