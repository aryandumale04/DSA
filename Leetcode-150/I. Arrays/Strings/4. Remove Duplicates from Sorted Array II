class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
       int k = 2;
       int uniqueIndex = 2;
       if (nums.size() <= 2) {
        return nums.size();
       }
       for (int i = 2; i < nums.size(); i++) {
        if (!(nums[i] == nums[uniqueIndex-1] && nums[i] == nums[uniqueIndex-2])) {
            //unique element 
            nums[uniqueIndex] = nums[i];
            uniqueIndex++;
            k++;
        }
       }
       return k;
    }
};