class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n = nums.size();
        map<int,int> freqCount;
        for (int i = 0; i < n; i++) {
            freqCount[nums[i]]++;
        }
       
        int majCount = n / 2;
        for (auto it : freqCount) {
            if (it.second> majCount) {
               return it.first;
            }
        }


    return -1;
    }
};