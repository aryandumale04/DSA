#include<unordered_map>
class Solution {
public:
    int romanToInt(string s) {
        int n = s.length();
        int num = 0;
        unordered_map<char,int> roman;
        roman['I'] = 1;
        roman['V'] = 5;
        roman['X'] = 10;
        roman['L'] = 50;
        roman['C'] = 100;
        roman['D'] = 500;
        roman['M'] = 1000;

        for (int i = 0; i < n - 1; i++) {
            if (roman[s[i]] < roman[s[i+1]]) {
                num -= roman[s[i]] ;
            } else {
                num +=  roman[s[i]];
            }
        }
        // last one is still left 
        num += roman[s[n-1]];
        return num;
    }
};