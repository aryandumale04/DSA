class Solution {
public:
    int evalRPN(vector<string>& tokens) {
        int n = tokens.size();
        stack<int> st;

        for(int i = 0; i < n; i++) {
            if(tokens[i] == "+" || tokens[i] == "-" || tokens[i] == "*" ||tokens[i] == "/" ) {
                //pop two numbers from the stack
                int num2 = st.top();
                st.pop();
                int num1 = st.top();
                st.pop();
                if(tokens[i] == "+") {
                    int result = num1 + num2;
                    st.push(result); 
                } else if(tokens[i] == "-") {
                    int result = num1 - num2;
                    st.push(result); 
                } else if(tokens[i] == "*") {
                    int result = num1 *  num2;
                    st.push(result); 
                } else {
                    int result = num1 / num2;
                    st.push(result); 
                }
            }
            else {
                // it is a number
                st.push(stoi(tokens[i]));
            }
        }
        return st.top();
    }

};